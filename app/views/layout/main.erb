<!doctype html>
<html lang="vi">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="google-site-verification" content="E5Oy0kgIZ3RFlBThplk1vr765X3yJbQY3PX8UxpwLo8">

    <title><%= yield_content(:title) { 'Build cấu hình máy tính - buildcauhinh' } %></title>
    <meta name="description" content="<%= yield_content(:description) { 'Cộng đồng build, chia sẻ cấu hình máy tính' } %>">

    <link rel="canonical" href="<%= yield_content(:canonical) { to('/') } %>">

    <meta property="og:title" content="<%= yield_content(:og_title) { 'Build cấu hình máy tính - buildcauhinh' } %>">
    <meta property="og:site_name" content="<%= yield_content(:og_site_name) { 'Build cấu hình máy tính - buildcauhinh' } %>">
    <meta property="og:description" content="<%= yield_content(:og_description) { 'Cộng đồng build, chia sẻ cấu hình máy tính' } %>">
    <meta property="og:url" content="<%= yield_content(:og_url) { to('/') } %>">
    <meta property="og:image" content="<%= yield_content(:og_image) { to(ASSETS['og.png']) } %>">
    <meta property="og:type" content="website">

    <link rel="dns-prefetch" href="//hanoicomputer.vn">
    <link rel="dns-prefetch" href="//hanoinew.vn">
    <link rel="dns-prefetch" href="//maihoang.com.vn">
    <link rel="dns-prefetch" href="//phongvu.vn">
    <link rel="dns-prefetch" href="//tandoanh.vn">
    <link rel="dns-prefetch" href="//vitinhnaman.com">

    <link rel="shortcut icon" type="image/x-icon" href="<%= ASSETS['favicon.ico'] %>">
    <link rel="apple-touch-icon" sizes="180x180" href="<%= ASSETS['touch_icon_180x180.png'] %>">
    <link rel="apple-touch-icon" sizes="152x152" href="<%= ASSETS['touch_icon_152x152.png'] %>">
    <link rel="apple-touch-icon" sizes="120x120" href="<%= ASSETS['touch_icon_120x120.png'] %>">
    <link rel="apple-touch-icon" sizes="76x76" href="<%= ASSETS['touch_icon_76x76.png'] %>">
    <link rel="icon" sizes="192x192" href="<%= ASSETS['touch_icon_192x192.png'] %>">

    <meta name="csrf-token" content="<%= Rack::Protection::AuthenticityToken.token(session) %>">
    <link rel="stylesheet" href="<%= ASSETS['main.css'] %>">
    <script src="<%= ASSETS['main.js'] %>" defer></script>

    <script async defer src="https://www.googletagmanager.com/gtag/js?id=UA-110209736-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-110209736-1', {
        'page_title': '<%= yield_content(:og_title) { "Build cấu hình máy tính - buildcauhinh" } %>',
        'page_url': '<%= yield_content(:canonical) { to("/") } %>'
      });
    </script>
  </head>
  <body>
    <%= erb :'shared/header' %>
    <%= yield %>
    <%= erb :'shared/footer' %>
    <%= erb :'shared/builder' %>
    <%= erb(:'shared/flash', locals: { message: yield_content(:success), success: true }) if content_for?(:success) %>
    <%= erb(:'shared/flash', locals: { message: yield_content(:error), success: false }) if content_for?(:error) %>
  </body>
</html>
